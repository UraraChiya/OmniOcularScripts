{
  "scopeName": "omniocular.injection",
  "injectionSelector": "L:text.xml.omniocular -comment",
  "patterns": [
    {
      "begin": "(<)(line|init)([^>]*)(>)",
      "beginCaptures": {
        "1": { "name": "punctuation.definition.tag.xml" },
        "2": { "name": "entity.name.tag.xml" },
        "3": { "patterns": [{ "include": "text.xml#tagStuff" }] },
        "4": { "name": "punctuation.definition.tag.xml" }
      },
      "end": "(</)(\\2)(>)",
      "endCaptures": {
        "1": { "name": "punctuation.definition.tag.xml" },
        "2": { "name": "entity.name.tag.xml" },
        "3": { "name": "punctuation.definition.tag.xml" }
      },
      "contentName": "source.js",
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    }
  ]
}